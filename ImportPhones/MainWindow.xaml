<Window x:Class="ImportPhones.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ImportPhones"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="1000">
    <Grid>
        <DataGrid x:Name="dgrResults" HorizontalAlignment="Left" Height="549" Margin="10,10,0,0" VerticalAlignment="Top" Width="819" AlternationCount="1" RowBackground="AliceBlue" AlternatingRowBackground="LightBlue" SelectionChanged="DgrResults_SelectionChanged"/>
        <Button x:Name="btnImportExcel" Content="Import Excel" FontSize="20" HorizontalAlignment="Left" Margin="845,160,0,0" VerticalAlignment="Top" Width="123" Height="70" Click="BtnImportExcel_Click"/>
        <Button x:Name="btnProcess" Content="Process" FontSize="20" HorizontalAlignment="Left" Margin="845,256,0,0" VerticalAlignment="Top" Width="123" Height="70" Click="BtnProcess_Click"/>
    </Grid>
</Window>
